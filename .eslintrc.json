{
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "parser": "babel-eslint",
      "rules": {
        "strict": 0
    },
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "jsx": true
        },
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "globals": {
        "$": true
    },
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [1, "single"],
        "semi": 0,
        "react/jsx-uses-vars": 1,
        "react/react-in-jsx-scope": 0,
        "no-unused-vars": 1,
        "react/jsx-key": 1,
        "no-extra-parens": 1,
        "no-extra-semi": 1,
        "no-inner-declarations": 1,
        "no-invalid-regexp": 2,
        "no-irregular-whitespace": 2,
        "no-sparse-arrays": 2,
        "eqeqeq": 2,
        "no-extra-bind": 1,
        "no-multi-spaces": 1,
        "no-redeclare": 2,
        "comma-dangle": [1, {
            "arrays": "only-multiline",
            "objects": "only-multiline",
            "functions": "ignore"
        }],
        "comma-spacing": [1, {
            "before": false,
            "after": true
        }],
        "no-const-assign": 2,
        "no-var": 2,
        "no-console": 0,
        "react/prop-types": 0,
        "indent": 0
    }
}